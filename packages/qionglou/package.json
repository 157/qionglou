{
  "name": "qionglou",
  "description": "An atomic component library for Vue 3",
  "version": "0.2.7",
  "keywords": [
    "qionglou",
    "ui",
    "vue",
    "javascript"
  ],
  "author": "Rhyme, Loyal <rhyme@npo.network>",
  "license": "MIT",
